    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Attach using Process Id",
            "type": "python",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": true
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: Cmd Args",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "args": ["--feed-folder-path","C:\\dev\\package-manager-feeds\\package-feeds\\all","--old-base-url","https://github.com/NI-MeasurementLink-Plug-Ins","--new-base-url","https://github.com/NI-Measurement-Plug-Ins"],
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
}